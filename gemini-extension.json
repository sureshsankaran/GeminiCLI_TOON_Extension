{
  "name": "TOON",
  "version": "1.0.0",
  "description": "Convert any JSON being sent to the LLM into TOON format and display token savings",
  "mcpServers": {
    "TOON": {
      "command": "bash",
      "args": ["${extensionPath}${/}servers${/}run.sh"]
    }
  },
  "contextFileName": "GEMINI.md",
  "commands": [
    {
      "path": "commands/toon/to_toon.TOML"
    },
    {
      "path": "commands/toon/to_toon_from_string.TOML"
    }
  ],
  "excludeTools": []
}
